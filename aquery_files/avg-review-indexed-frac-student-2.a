CREATE TABLE my_table (student_name STRING, review_score FLOAT)

LOAD DATA INFILE "indexed_reviews_output.csv"
INTO TABLE my_table
FIELDS TERMINATED BY "|"

SELECT DISTINCT	student_name, AVG(review_score) AS avg_score
FROM 	my_table
	ASSUMING ASC student_name
WHERE student_name = "Connor Melvin Underwood"

INTO OUTFILE "reviews_output.csv"
FIELDS TERMINATED BY "|"
